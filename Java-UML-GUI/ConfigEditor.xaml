<Window x:Class="Java_UML_GUI.ConfigEditor"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Java_UML_GUI"
        mc:Ignorable="d"
        Title="ConfigEditor" Height="300" Width="300">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Button x:Name="LoadButton" Content="Load Config" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="75"/>
        <Button x:Name="SaveButton" Content="Save Config" HorizontalAlignment="Right" Margin="0,10,10,0" VerticalAlignment="Top" Width="75" Grid.Column="2"/>
        <Label x:Name="label" Content="Label" HorizontalAlignment="Left" Margin="31,29,0,0" Grid.Row="1" VerticalAlignment="Top"/>
        <TextBox x:Name="textBox" Grid.Row="1" HorizontalAlignment="Left" Height="23" Margin="50,29,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="85"/>

    </Grid>
</Window>
