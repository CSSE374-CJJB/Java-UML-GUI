<Window x:Class="Java_UML_GUI.Loading"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Java_UML_GUI"
        mc:Ignorable="d"
        Title="Loading" Height="50" Width="300" WindowStyle="None" ResizeMode="NoResize" WindowStartupLocation="CenterScreen">
    <Grid>
        <ProgressBar x:Name="progressBar" HorizontalAlignment="Stretch" Height="40" Margin="5,5,5,5" VerticalAlignment="Center" Minimum="0" Maximum="100" Value="0"/>
        <Label x:Name="statusLabel" Content="Loading" HorizontalAlignment="Center" VerticalAlignment="Center"/>
    </Grid>
</Window>
